let value = 10;

class Person {
  let mut name;
  let mut age;
}

fn print_person(person) {
  print("Name: " + str(person.name) + "; Age: " + str(person.age));
}

let dylan = Person();
let abby = Person();

dylan.name = "Dylan";
dylan.age = 25;
abby.name = "Abby";
abby.age = 23;

print_person(dylan);
print_person(abby);
