fn itemToStr(item) {
    fn toStr(item) {
        return str(item);
    }

    return toStr(item);
}

print(itemToStr(12345));


fn otherToStr(a) {
    fn toStr(b) {
        return str(b);
    }

    return toStr(a);
}

print(otherToStr(67890));

