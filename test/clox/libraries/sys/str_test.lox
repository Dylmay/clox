fn fn_to_print() {}
let empty_val;

fn print_using_str(item) {
    let to_str = str(item);

    print(to_str);
}

class Cls {}

assert(str(true) == "true");
assert(str(Cls) == "Cls")
assert(str(false) == "false");
assert(str(nil) == "nil");
assert(str(123_456) == "123456");
assert(str("hello world") == "hello world");
assert(str("\"string\"" == "\"string\""));
assert(str() == "");

print_using_str("Hello World");
print_using_str(nil);
print_using_str(123);
print_using_str(true);
print_using_str(false);
print_using_str();
print_using_str(fn_to_print);
print_using_str(empty_val);

